{
  "languageserver": {
    "csharp": {
      "command": "dotnet",
      "args": [
        "/usr/local/Cellar/omnisharp/1.35.3/libexec/omnisharp/OmniSharp.exe",
        "--languageserver"
      ],
      "rootPatterns": ["*.sln", "*.csproj"],
      "filetypes": ["cs"],
      "initializationOptions": {
        "omnisharp": {
          "useModernNet": true
        }
      }
    },
    "typescript": {
      "command": "typescript-language-server",
      "args": ["--stdio"],
      "filetypes": [
        "typescript",
        "typescriptreact",
        "typescript.tsx",
        "javascript",
        "javascriptreact",
        "javascript.jsx"
      ]
    },
    "python": {
      "command": "pyright-langserver",
      "args": ["--stdio"],
      "filetypes": ["python"]
    }
  },
  "coc.preferences": {
    "documentHighlight.priority": -11
  },
  "prettier": {
    "disableLanguages": [],
    "useTabs": false,
    "tabWidth": 2,
    "singleQuote": true
  },
  "python.formatting": {
    "provider": "black",
    "blackArgs": ["--line-length", "88"]
  },
  "csharp.format": {
    "enable": true
  }
}
